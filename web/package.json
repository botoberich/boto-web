    "version": "1.0.0",
    "author": "@kangaroo",
    "scripts": {
        "develop": "gatsby develop",
        "start": "run-p start:**",
        "start:app": "npm run develop",
        "start:type-check": "tsc --noEmit -w",
        "storybook": "start-storybook",
        "build": "run-p build:**",
